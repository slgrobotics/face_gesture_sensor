<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>face_gesture_sensor</name>
  <version>0.1.0</version>
  <description>Face and Gesture detection node</description>
  <!-- based on https://github.com/DFRobot/DFRobot_GestureFaceDetection/tree/master/python/raspberrypi  -->
  <maintainer email="slg@quakemap.com">Sergei Grichine</maintainer>
  <!-- preserving original DFRobot license -->
  <license>MIT</license>

  <depend>rclpy</depend>
  <depend>setuptools</depend>
  
  <exec_depend>python3-serial</exec_depend>

  <buildtool_depend>ament_python</buildtool_depend>

  <export>
    <build_type>ament_python</build_type>
  </export>
</package>
